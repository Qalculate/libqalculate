      <argument type="number" index="1">
        <title>Value</title>
        <handle_vector>true</handle_vector>
      </argument>
      <argument type="integer" index="2">
        <title>Bits (integer part)</title>
        <min>0</min>
      </argument>
      <argument type="integer" index="3">
        <title>Bits (fraction part)</title>
        <min>0</min>
      </argument>
