#: ../src/qalc.cc:5008
msgid "Commands for RPN mode:"
msgstr "Kommandon för RPN-läget:"

#: ../src/qalc.cc:5009
msgid "STATE"
msgstr "LÄGE"

#: ../src/qalc.cc:5012 ../src/qalc.cc:5014
msgid "INDEX"
msgstr "INDEX"

#: ../src/qalc.cc:5013 ../src/qalc.cc:5016
