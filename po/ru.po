#: ../src/qalc.cc:4847
msgid "Commands for RPN mode:"
msgstr "Команды режима ПОЛИЗ:"

#: ../src/qalc.cc:4848
msgid "STATE"
msgstr "МЕСТО"

#: ../src/qalc.cc:4851 ../src/qalc.cc:4853
msgid "INDEX"
msgstr " ИНДЕКС"

#: ../src/qalc.cc:4852 ../src/qalc.cc:4855
